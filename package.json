{
  "name": "vpp-colombia",
  "version": "1.0.0",
  "author": "Gabriel Pinto & Vivian Gómez",
  "license": "MIT",
  "description": "Esta aplicación expone proyectos públicos de inversión con el fin de que los ciudadanos le puedan hacer un seguimiento correcto al desarrollo e implementación de estos.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "dev": "nodemon index.js",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "engines": {
    "node": "8.11.1"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "keywords": [
    "Colombia",
    "Proyectos",
    "públicos",
    "Proyectos",
    "Veeduría",
    "Ciudadanos",
    "Participación",
    "ciudadana"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/glpinto10/veeduria-proyectos-publicos.git"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.14",
    "aws-sdk": "^2.502.0",
    "bad-words": "^3.0.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "mongoose": "^5.7.5",
    "node-fetch": "^2.6.0",
    "prettier-eslint-cli": "^4.7.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1"
  }
}
